{
  "editor.fontSize": 16,
  "workbench.colorTheme": "Dracula",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.profiles.windows": {
    "PowerShell -NoLogo": {
      "source": "PowerShell",
      "args": ["-NoLogo"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell -NoLogo",
  "mesonbuild.configureOnOpen": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.defaultInterpreterPath": "C:\\Python39\\python.exe",
  "powershell.buttons.showRunButtons": false,
  "powershell.integratedConsole.showOnStartup": false,
  "autoHide.sideBarDelay": 0,
  "autoHide.panelDelay": 0,
  "editor.fontFamily": "Consolas, 'Courier New', monospace, Cascadia Code PL",
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "[rust]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
	"vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "commands":["editor.action.clipboardPasteAction"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
      {
          "before": ["gcc"],
          "commands": [
            "editor.action.commentLine"
            ]
      },
      {
        "before": ["gc"],
        "commands": [
          "editor.action.blockComment"
        ]
      }
  ],
  "multiCommand.commands": [
    {
      "command": "multiCommand.saveAndCloseEditor",
      "sequence": [
        "workbench.action.files.save",
        "workbench.action.closeActiveEditor"
      ]
    }
  ],
  "tabnine.experimentalAutoImports": true,
  "vim.useSystemClipboard": true,
  "clangd.path": "c:\\Users\\Bay\\AppData\\Roaming\\Code\\User\\globalStorage\\llvm-vs-code-extensions.vscode-clangd\\install\\12.0.0\\clangd_12.0.0\\bin\\clangd.exe",
  "extensions.ignoreRecommendations": true,
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "none",
  "rust-analyzer.inlayHints.enable": false,
  "editor.inlineSuggest.enabled": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "powershell.promptToUpdatePowerShell": false
}
